<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/jc_header_text_white">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/header_type_one_panel"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/main_color">
        <LinearLayout
            android:id="@+id/lly_reward_record_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:padding="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/header_type_one_back_arrow"
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:paddingLeft="10dip"
                android:adjustViewBounds="true"
                android:src="@drawable/arrow_left_white"/>
        </LinearLayout>
        <TextView
            android:id="@+id/header_type_one_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/delivery_records"
            android:textColor="@color/white"
            android:textSize="18sp"/>
        <LinearLayout
            android:id="@+id/lly_reward_record_select"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dip"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dip"
            android:layout_alignParentRight="true">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:adjustViewBounds="true"
                android:src="@drawable/cx_w" />

        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
        <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:background="@color/white"
            android:layout_marginTop="0.5dip"
            android:orientation="horizontal"
            android:weightSum="2"
            android:visibility="gone">
            <LinearLayout
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">
                <Spinner
                    android:id="@+id/sp_delivery_one"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="14dip"
                    android:textColor="#333333"
                    android:visibility="visible"
                    android:background="@color/white"
                    android:layout_marginRight="10dip"
                    android:dropDownWidth="match_parent"
                    android:dropDownVerticalOffset="33dp"
                    android:layout_marginLeft="10dip"/>
                <ImageView
                    android:layout_width="7dip"
                    android:layout_height="12dip"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/arrow_right_small"
                    android:layout_marginRight="20dip"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center">
                <Spinner
                    android:id="@+id/sp_delivery_two"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textSize="14dip"
                    android:textColor="#333333"
                    android:visibility="visible"
                    android:spinnerMode="dropdown"
                    android:background="@color/white"
                    android:layout_marginRight="10dip"
                    android:dropDownVerticalOffset="33dp"
                    android:dropDownWidth="match_parent"
                    android:layout_marginLeft="10dip"/>
                <ImageView
                    android:layout_width="7dip"
                    android:layout_height="12dip"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/arrow_right_small"
                    android:layout_marginRight="20dip"/>
            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/jc_header_text_white">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/jc_header_text_white">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:layout_marginLeft="20dip"
                    android:layout_marginRight="20dip"
                    android:layout_marginTop="15dip"
                    android:gravity="center"
                    android:visibility="visible"
                    android:layout_marginBottom="5dip"
                    android:background="@drawable/reward_et_bg"
                    android:focusable="true"
                    android:focusableInTouchMode="true">
                    <EditText
                        android:id="@+id/tv_receiving_select_retrieval"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:layout_marginLeft="15dip"
                        android:hint="@string/please_recipient_retrieval"
                        android:textSize="14dip"
                        android:gravity="center"
                        android:background="@null"/>
                    <ImageView
                        android:id="@+id/img_receiving_select_retrieval"
                        android:layout_width="wrap_content"
                        android:layout_height="20dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/cx"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="20dip"/>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <com.jc.lottery.view.XListView
                        android:id="@+id/xlv_delivery_record"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:cacheColorHint="@null"
                        android:divider="@null"
                        android:scrollbars="none"
                        android:background="@color/jc_header_text_white"/>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:id="@+id/lly_receiving_nodata"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:visibility="gone"
                        android:layout_marginTop="20dip">
                        <ImageView
                            android:id="@+id/img_reward_nodata"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_widthPercent="35%sw"
                            android:adjustViewBounds="true"
                            android:layout_gravity="center"
                            android:src="@drawable/no_record"
                            />
                    </com.zhy.android.percent.support.PercentLinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lly_receiving_select"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#99000000"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/lly_receiving_click"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/choose_game"
                    android:textColor="@color/black"
                    android:textSize="16dip"
                    android:gravity="center|left"
                    android:layout_marginTop="15dip"
                    android:layout_marginBottom="15dip"
                    android:layout_marginLeft="15dip"/>
                <LinearLayout
                    android:id="@+id/lly_receiving_select_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip">
                    <com.jc.lottery.view.FlowLayout
                        android:id="@+id/flow_one"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white" />
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="#F3F3F3"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:layout_marginTop="15dip"
                    android:visibility="gone"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/selection_status"
                    android:textColor="@color/black"
                    android:textSize="16dip"
                    android:gravity="center|left"
                    android:layout_marginTop="15dip"
                    android:layout_marginBottom="15dip"
                    android:layout_marginLeft="15dip"/>
                <LinearLayout
                    android:id="@+id/lly_receiving_select_container_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginBottom="20dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip">
                    <com.jc.lottery.view.FlowLayout
                        android:id="@+id/flow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white" />
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/time_choice"
                    android:textColor="@color/black"
                    android:textSize="16dip"
                    android:gravity="center|left"
                    android:layout_marginTop="15dip"
                    android:layout_marginBottom="15dip"
                    android:layout_marginLeft="15dip"/>
                <LinearLayout
                    android:id="@+id/lly_receiving_select_container_three"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:gravity="center_vertical">
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/tv_receiving_select_start"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text=""
                            android:hint="@string/please_choose"
                            android:textSize="12dip"
                            android:textColor="#888888"
                            android:gravity="center"/>

                        <ImageView
                            android:layout_width="12dip"
                            android:layout_height="wrap_content"
                            android:src="@drawable/time_right"
                            android:layout_marginRight="15dip"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="--"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:textColor="#888888"
                        android:visibility="invisible"/>
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/tv_receiving_select_end"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text=""
                            android:hint="@string/please_choose"
                            android:textSize="12dip"
                            android:textColor="#888888"
                            android:gravity="center"/>

                        <ImageView
                            android:layout_width="12dip"
                            android:layout_height="wrap_content"
                            android:src="@drawable/time_right"
                            android:layout_marginRight="15dip"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dip"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginTop="20dip">
                    <Button
                        android:id="@+id/btn_receiving_select_reset"
                        android:layout_width="0dip"
                        android:layout_height="35dip"
                        android:layout_weight="1"
                        android:background="@drawable/scanner_shape_bg"
                        android:text="@string/reset_time"
                        android:textColor="@color/white"
                        android:textSize="16dip"
                        android:layout_marginLeft="20dip"
                        android:layout_marginRight="20dip" />

                    <Button
                        android:id="@+id/btn_receiving_select_submit"
                        android:layout_width="0dip"
                        android:layout_height="35dip"
                        android:layout_weight="1"
                        android:background="@drawable/scanner_shape_bg"
                        android:text="@string/query"
                        android:textColor="@color/white"
                        android:textSize="16dip"
                        android:layout_marginRight="20dip" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lly_delivery_pop"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:visibility="gone"
            android:background="#33000000"
            android:orientation="vertical"
            android:paddingBottom="10dip">
            <LinearLayout
                android:id="@+id/lly_delivery_pop_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dip"
                android:layout_marginRight="40dip"
                android:background="@drawable/pop_delivery_management_bg"
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:visibility="visible">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="32dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/logistics_information"
                        android:textSize="14dip"
                        android:textColor="@color/white"
                        android:background="@drawable/delivery_management_lly_bg"
                        android:gravity="center"
                        android:paddingLeft="35dip"
                        android:paddingRight="35dip"/>
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center_vertical|right">
                        <ImageView
                            android:id="@+id/img_delivery_pop_no"
                            android:layout_width="42dip"
                            android:layout_height="32dip"
                            android:src="@drawable/guanbi"
                            android:padding="8dip"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/receiving_detail_lly_bg"
                    android:layout_marginTop="20dip"
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:layout_marginBottom="20dip">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="35dip"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:id="@+id/tv_delivery_pop_one"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="发件人电话"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="#F3F3F3"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="35dip"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:id="@+id/tv_delivery_pop_two"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="left|center_vertical"
                            android:text="发件人姓名"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                        <View
                            android:layout_width="1dip"
                            android:layout_height="match_parent"
                            android:background="#F3F3F3"/>
                        <TextView
                            android:id="@+id/tv_delivery_pop_three"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="left|center_vertical"
                            android:text="司机姓名"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="#F3F3F3"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="35dip"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:id="@+id/tv_delivery_pop_four"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="司机电话"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="#F3F3F3"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="35dip"
                        android:orientation="horizontal"
                        android:weightSum="1">
                        <TextView
                            android:id="@+id/tv_delivery_pop_five"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="left|center_vertical"
                            android:text="车牌号"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dip"
                        android:background="#F3F3F3"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="35dip"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:id="@+id/tv_delivery_pop_seven"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="left|center_vertical"
                            android:text="寄件时间"
                            android:textSize="12dip"
                            android:textColor="#333333"
                            android:layout_marginLeft="5dip"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
