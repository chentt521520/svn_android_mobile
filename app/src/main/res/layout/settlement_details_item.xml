<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="5dip"
        android:background="@color/white">
        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/lly_settle_details_click"
            android:layout_width="match_parent"
            android:layout_height="45dip"
            android:orientation="horizontal"
            android:gravity="center_vertical">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="20dip"
                android:adjustViewBounds="true"
                android:layout_marginLeft="20dip"
                android:visibility="gone"/>
            <View
                android:id="@+id/view_settle_details_oval"
                android:layout_width="8dip"
                android:layout_height="8dip"
                android:background="@drawable/settle_details_item_oval_red"
                android:layout_marginLeft="20dip"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ticket_number"
                android:textSize="14dip"
                android:textColor="@color/black"
                app:layout_textSizePercent="4%sw"
                android:layout_marginLeft="5dip" />

            <TextView
                android:id="@+id/tv_settle_details_order"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:textSize="15dip"
                app:layout_textSizePercent="4%sw"
                android:layout_marginLeft="10dip"
                android:singleLine="true" />

            <TextView
                android:id="@+id/tv_settle_details_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/wait_shenhe"
                android:textSize="10dip"
                app:layout_textSizePercent="3%sw"
                android:textColor="@color/white"
                android:paddingTop="3dip"
                android:paddingBottom="3dip"
                android:paddingLeft="10dip"
                android:paddingRight="10dip"
                android:layout_marginLeft="5dip"
                android:visibility="gone"/>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right">
                <ImageView
                    android:id="@+id/img_settle_details_arrow"
                    android:layout_width="10dip"
                    android:layout_height="10dip"
                    android:adjustViewBounds="true"
                    android:src="@drawable/arrow_bottom"
                    android:layout_marginRight="20dip"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
        </com.zhy.android.percent.support.PercentLinearLayout>
        <View
            android:id="@+id/view_settle_details_line"
            android:layout_width="match_parent"
            android:layout_height="1dip"
            android:background="#F3F3F3"
            android:layout_marginLeft="5dip"
            android:layout_marginRight="5dip"
            android:visibility="gone"/>
        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/lly_settle_details_order"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:gravity="center_vertical"
            android:layout_marginLeft="20dip">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/settlement_order_number"
                android:textSize="14dip"
                app:layout_textSizePercent="4%sw"
                android:textColor="#666666"/>
            <TextView
                android:id="@+id/tv_settle_details_content_order"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="14dip"
                app:layout_textSizePercent="4%sw"
                android:textColor="#666666"
                android:layout_marginLeft="10dip"
                android:singleLine="true"
                android:ellipsize="end"/>
        </com.zhy.android.percent.support.PercentLinearLayout>
        <com.zhy.android.percent.support.PercentLinearLayout
            android:id="@+id/lly_settle_details_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:orientation="horizontal"
            android:visibility="visible">
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="20dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/game_type"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_game"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"
                        android:layout_marginLeft="10dip"
                        android:singleLine="true"
                        android:ellipsize="end"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="20dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/auditor"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_auditor"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"
                        android:layout_marginLeft="10dip"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="20dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/audit_time"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"
                        android:layout_marginLeft="10dip"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/winner"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_merchant"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"
                        android:layout_marginLeft="10dip"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/reward_time"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_cash"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"
                        android:layout_marginLeft="10dip"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dip">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/reward_money"
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="#666666"/>
                    <TextView
                        android:id="@+id/tv_settle_details_content_money"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textSize="14dip"
                        app:layout_textSizePercent="4%sw"
                        android:textColor="@color/main_color"
                        android:layout_marginLeft="10dip" />
                </com.zhy.android.percent.support.PercentLinearLayout>
            </com.zhy.android.percent.support.PercentLinearLayout>
        </com.zhy.android.percent.support.PercentLinearLayout>
    </com.zhy.android.percent.support.PercentLinearLayout>
</com.zhy.android.percent.support.PercentLinearLayout>
