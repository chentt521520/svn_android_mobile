<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#F3F3F3">
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="48dp"
            android:background="@color/main_color"
            android:gravity="center_vertical">

        <LinearLayout
            android:id="@+id/lly_reward_record_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dip">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:paddingLeft="10dip"
                android:adjustViewBounds="true"
                android:src="@drawable/arrow_left_white" />

        </LinearLayout>
            <TextView
                android:id="@+id/tv_recharge_detail_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_centerVertical="true"
                android:text="@string/receiving_details"
                android:textColor="@color/white"
                android:textSize="18sp" />

        </RelativeLayout>

        <com.scwang.smartrefresh.layout.SmartRefreshLayout
            android:id="@+id/srl_receiving"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.scwang.smartrefresh.layout.header.ClassicsHeader
                android:id="@+id/header_receiving"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <ScrollView
                android:id="@+id/sv_detail"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center">
                            <TextView
                                android:id="@+id/tv_receiving_two"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/book_number"
                                android:textSize="16dip"
                                android:textColor="@color/black"
                                android:layout_marginTop="10dip"
                                android:layout_marginBottom="10dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_real_moneys"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text=""
                                android:textSize="24dip"
                                android:textStyle="bold"
                                android:textColor="@color/main_color"
                                android:layout_marginBottom="5dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_real_state"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="@string/successful_trade"
                                android:textSize="12dip"
                                android:textColor="@color/black"
                                android:layout_marginBottom="5dip"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dip"
                            android:gravity="center_vertical"
                            android:background="#f3f3f3"
                            android:visibility="gone">
                            <View
                                android:layout_width="3dip"
                                android:layout_height="15dip"
                                android:background="@color/main_color"
                                android:layout_marginLeft="15dip"
                                android:layout_marginTop="0.5dip"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/order_information"
                                android:textSize="16dip"
                                android:textColor="@color/black"
                                android:layout_marginLeft="5dip"/>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/payment_method"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_payment_method"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/number"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_four"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/order_money"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_money"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/actual_payment_amount"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_real_money"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="visible"
                            android:layout_marginTop="5dip"
                            android:layout_marginBottom="5dip"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/payment_status"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_three"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/tv_receiving_state"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_six"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="@color/main_color"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/ticket_number"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_one"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="#333333"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text="@string/order_time"
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_seven"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="#333333"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#F3F3F3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"
                            android:visibility="gone"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dip"
                            android:orientation="horizontal">
                            <TextView
                                android:id="@+id/tv_receiving_time"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="#999999"
                                android:layout_marginLeft="15dip"/>
                            <TextView
                                android:id="@+id/tv_receiving_five"
                                android:layout_width="0dip"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center|right"
                                android:text=""
                                android:textSize="12dip"
                                android:textColor="#333333"
                                android:layout_marginRight="15dip"/>
                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lly_receiving_pop"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:visibility="gone"
                        android:layout_marginLeft="20dip"
                        android:layout_marginRight="20dip"
                        android:layout_marginBottom="20dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/scanning_lottery_tickets"
                            android:textSize="14dip"
                            android:textColor="@color/grad"
                            android:layout_marginTop="14dip"
                            android:gravity="center"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center">
                            <ImageView
                                android:id="@+id/img_lottery_pop_one"
                                android:layout_width="210dip"
                                android:layout_height="210dip"
                                android:adjustViewBounds="true" />
                            <LinearLayout
                                android:id="@+id/lly_lottery_pop_zoom"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:gravity="center">
                                <ImageView
                                    android:layout_width="15dip"
                                    android:layout_height="wrap_content"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/cx" />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/click_to_zoom_in"
                                    android:textSize="12dip"
                                    android:textColor="@color/grad"
                                    android:layout_marginLeft="2dip"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/lly_receiving_activation"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:background="@color/white"
                        android:visibility="gone">
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dip"
                            android:background="#f3f3f3">
                        </View>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="40dip"
                            android:gravity="center_vertical"
                            android:background="@color/white">
                            <View
                                android:layout_width="3dip"
                                android:layout_height="10dip"
                                android:background="#1577FF"
                                android:layout_marginLeft="15dip"
                                android:layout_marginTop="0.5dip"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/activation_information"
                                android:textSize="14dip"
                                android:textColor="#1577FF"
                                android:layout_marginLeft="5dip"/>
                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dip"
                            android:background="#f3f3f3"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip">
                        </View>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_margin="10dip">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="35dip"
                                android:orientation="horizontal"
                                android:weightSum="4">
                                <TextView
                                    android:id="@+id/tv_receiving_item_no"
                                    android:layout_width="30dip"
                                    android:layout_height="match_parent"
                                    android:gravity="center"
                                    android:text="@string/xuhao"
                                    android:textSize="12dip"
                                    android:textColor="#333333" />
                                <TextView
                                    android:id="@+id/tv_receiving_item_one"
                                    android:layout_width="0dip"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/batch_number"
                                    android:textSize="12dip"
                                    android:textColor="#333333" />
                                <TextView
                                    android:id="@+id/tv_receiving_item_two"
                                    android:layout_width="0dip"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/book_numbers"
                                    android:textSize="12dip"
                                    android:textColor="#333333" />
                                <TextView
                                    android:id="@+id/tv_receiving_item_type"
                                    android:layout_width="0dip"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/activation_state"
                                    android:textSize="12dip"
                                    android:textColor="#333333" />
                                <TextView
                                    android:id="@+id/tv_receiving_item_time"
                                    android:layout_width="0dip"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:gravity="center"
                                    android:text="@string/activation_time"
                                    android:textSize="12dip"
                                    android:textColor="#333333" />
                            </LinearLayout>
                            <ListView
                                android:id="@+id/lv_receiving_detail"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:divider="@null"
                                android:layout_marginLeft="1dip"
                                android:layout_marginRight="1dip"
                                android:layout_marginBottom="1dip"/>
                        </LinearLayout>
                    </LinearLayout>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:id="@+id/lly_receiving_record_order"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginTop="20dip"
                        android:layout_marginBottom="10dip"
                        android:visibility="gone">
                        <Button
                            android:id="@+id/bt_receiving_record_order"
                            android:layout_width="match_parent"
                            android:layout_height="32dip"
                            android:background="@drawable/button_add_zhuce"
                            android:text="@string/order_payment"
                            android:textColor="@color/white"
                            android:textSize="14dip"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"/>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:id="@+id/lly_receiving_record_cancel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginTop="10dip"
                        android:layout_marginBottom="20dip"
                        android:visibility="gone">
                        <Button
                            android:id="@+id/bt_receiving_record_cancel"
                            android:layout_width="match_parent"
                            android:layout_height="32dip"
                            android:background="@drawable/button_add_zhuce"
                            android:text="@string/closing_order"
                            android:textColor="@color/white"
                            android:textSize="14dip"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"/>
                    </com.zhy.android.percent.support.PercentLinearLayout>

                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:id="@+id/lly_receiving_record_audit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:layout_marginTop="10dip"
                        android:layout_marginBottom="20dip"
                        android:visibility="gone">
                        <Button
                            android:id="@+id/bt_receiving_record_a_audit"
                            android:layout_width="0dip"
                            android:layout_height="48dip"
                            android:layout_weight="1"
                            app:layout_heightPercent="6%sh"
                            android:background="@drawable/btn_delivery_inout_back_bg"
                            android:text="@string/to_examine"
                            android:textColor="@color/white"
                            android:textSize="14dip"
                            android:layout_marginLeft="15dip"
                            android:layout_marginRight="15dip"/>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                </LinearLayout>
            </ScrollView>
            <com.scwang.smartrefresh.layout.footer.ClassicsFooter
                android:id="@+id/footer_receiving"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </com.scwang.smartrefresh.layout.SmartRefreshLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lly_receiving_pops"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:background="#33000000"
        android:orientation="vertical"
        android:paddingBottom="10dip"
        android:visibility="gone">
        <LinearLayout
            android:id="@+id/lly_receiving_pop_ones"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/pop_receiving_bg"
            android:orientation="vertical"
            android:gravity="center_horizontal"
            android:visibility="visible"
            android:layout_marginLeft="40dip"
            android:layout_marginRight="40dip">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="45dip"
                android:gravity="center"
                android:text="@string/to_examine"
                android:textSize="16dip"
                android:textColor="@color/white"
                android:background="@drawable/pop_receiving_top_bg"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dip"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <LinearLayout
                    android:id="@+id/lly_receiving_select_yes"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/sh_yes"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/adopt"
                        android:textSize="20dip"
                        android:textColor="#74B55F"
                        android:layout_marginLeft="5dip"/>
                    <ImageView
                        android:id="@+id/img_receiving_select_yes"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginLeft="15dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/settle_yes"
                        android:visibility="visible"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/lly_receiving_select_no"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="40dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/sh_no"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/reject"
                        android:textSize="20dip"
                        android:textColor="#D24A44"
                        android:layout_marginLeft="5dip"/>
                    <ImageView
                        android:id="@+id/img_receiving_select_no"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_marginLeft="15dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/settle_no"
                        android:visibility="visible"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="45dip"
                android:weightSum="2"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:background="@drawable/pop_receiving_btn_bg">
                <Button
                    android:id="@+id/bt_receiving_pop_nos"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/str_cancel"
                    android:textColor="@color/white"
                    android:textSize="16dip"
                    android:background="@null"/>
                <View
                    android:layout_width="0.5dip"
                    android:layout_height="match_parent"
                    android:background="@color/white"/>
                <Button
                    android:id="@+id/bt_receiving_pop_yess"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/confirm"
                    android:textColor="@color/white"
                    android:textSize="16dip"
                    android:background="@null"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
