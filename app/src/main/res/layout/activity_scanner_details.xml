<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/manual_scanner_bgs">
        <RelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/header_type_one_panel"
            android:layout_width="match_parent"
            android:layout_height="48dp">

            <LinearLayout
                android:id="@+id/lly_manual_scanner_back"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:padding="5dip"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/header_type_one_back_arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="23dip"
                    android:paddingLeft="10dip"
                    android:adjustViewBounds="true"
                    android:src="@drawable/arrow_left_white"/>

            </LinearLayout>

            <TextView
                android:id="@+id/header_type_one_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/bonus_details"
                android:textColor="#ffffff"
                android:textSize="18sp"/>
        </RelativeLayout>
        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dip"
            android:layout_weight="1"
            android:orientation="vertical"
            android:background="@drawable/scanner_details_bg"
            app:layout_marginLeftPercent="3%sw"
            app:layout_marginRightPercent="3%sw">
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_marginTopPercent="3%sh"
                app:layout_marginBottomPercent="1%sh"
                android:gravity="center">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="2%sh"
                    android:adjustViewBounds="true"
                    android:src="@drawable/bt02"/>
                <TextView
                    android:id="@+id/tv_scanner_details_game"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4.5%sw"
                    android:textColor="#3B8CFE"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="2%sh"
                    android:adjustViewBounds="true"
                    android:src="@drawable/bt01"/>
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/ticket_number"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/name_of_recipient"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_recipient"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/mame_of_the_recipients_superior"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_recipient_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:id="@+id/lly_scanner_details_state"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/winstates"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_win_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/reward_status"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_cash_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/activation_state"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_active_state"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dip"
                app:layout_marginLeftPercent="7%sw"
                app:layout_marginRightPercent="7%sw"
                android:background="@color/jc_header_text_white"/>
            <TextView
                android:id="@+id/tv_scanner_details_order"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:text=""
                app:layout_textSizePercent="4.2%sw"
                android:textColor="@color/black"
                android:letterSpacing="0.1"
                app:layout_marginLeftPercent="8%sw"
                app:layout_marginRightPercent="8%sw"
                android:gravity="center"/>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_marginTopPercent="1%sh"
                app:layout_marginBottomPercent="1%sh"
                android:gravity="center">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="2%sh"
                    android:adjustViewBounds="true"
                    android:src="@drawable/bt02"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/winning_information"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4.5%sw"
                    android:textColor="#3B8CFE"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="2%sh"
                    android:adjustViewBounds="true"
                    android:src="@drawable/bt01"/>
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:id="@+id/lly_scanner_details_money"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="22.5%sh"
                android:gravity="center"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/tv_scanner_details_money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="6%sw"
                    android:textColor="@color/delivery_record_tx_color"
                    android:letterSpacing="0.1" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/zhongjiangjine"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="#666666"
                    android:letterSpacing="0.1"
                    android:layout_marginTop="5dip"/>
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:id="@+id/lly_scanner_details_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/zhongjiangjine"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_active_money"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:id="@+id/lly_scanner_details_two"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/winstates"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:id="@+id/tv_scanner_details_win_states"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_heightPercent="4.5%sh"
                android:gravity="center_vertical"
                android:visibility="gone">
                <TextView
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="奖期"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="3%sw"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1235456"
                    android:textSize="16dip"
                    app:layout_textSizePercent="4%sw"
                    android:textColor="@color/black"
                    android:letterSpacing="0.1"
                    app:layout_marginLeftPercent="3%sw"
                    app:layout_marginRightPercent="8%sw"/>
            </com.zhy.android.percent.support.PercentLinearLayout>

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical|bottom">
                <Button
                    android:id="@+id/btn_scanner_details_submit"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    app:layout_heightPercent="6%sh"
                    android:text="@string/confirm_the_award"
                    android:textColor="@color/white"
                    app:layout_textSizePercent="2%sh"
                    app:layout_marginLeftPercent="8%sw"
                    app:layout_marginRightPercent="8%sw"
                    app:layout_marginBottomPercent="3%sh"
                    android:background="@drawable/scanner_shape_bg"/>
            </com.zhy.android.percent.support.PercentLinearLayout>
        </com.zhy.android.percent.support.PercentLinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/please_consult"
            android:textColor="@color/material_yellow"
            app:layout_textSizePercent="4%sw"
            app:layout_marginTopPercent="2%sh"
            app:layout_marginBottomPercent="2%sh"
            android:gravity="center"/>
    </com.zhy.android.percent.support.PercentLinearLayout>

</LinearLayout>
