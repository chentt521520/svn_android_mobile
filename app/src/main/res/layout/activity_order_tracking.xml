<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/main_tab_wei">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/header_type_one_panel"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/main_color">
        <LinearLayout
            android:id="@+id/lly_track_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:padding="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/header_track_back_arrow"
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:paddingLeft="10dip"
                android:adjustViewBounds="true"
                android:src="@drawable/arrow_left_white"/>
        </LinearLayout>
        <TextView
            android:id="@+id/header_type_one_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/order_tracking"
            android:textColor="@color/white"
            android:textSize="18sp"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dip"
        android:layout_marginLeft="20dip"
        android:layout_marginRight="20dip"
        android:layout_marginTop="15dip"
        android:gravity="center|left"
        android:visibility="visible"
        android:layout_marginBottom="5dip"
        android:background="@drawable/order_tracking_et_bg"
        android:focusable="true"
        android:focusableInTouchMode="true">
        <ImageView
            android:id="@+id/img_receiving_select_order"
            android:layout_width="wrap_content"
            android:layout_height="20dip"
            android:adjustViewBounds="true"
            android:src="@drawable/settlement_qr"
            android:layout_marginLeft="20dip"
            android:layout_marginRight="5dip"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <EditText
                android:id="@+id/et_track_select"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:hint="@string/batch_number"
                android:textSize="14dip"
                android:gravity="center"
                android:background="@null"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="--"
                android:textSize="20dip"
                android:textColor="@color/white"/>
            <EditText
                android:id="@+id/et_track_select_two"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:hint="@string/book_numbers"
                android:textSize="14dip"
                android:gravity="center"
                android:background="@null"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/tv_track_select"
            android:layout_width="wrap_content"
            android:layout_height="20dip"
            android:gravity="center"
            android:layout_marginRight="20dip"
            android:adjustViewBounds="true"
            android:src="@drawable/tracking_select"/>
    </LinearLayout>

    <com.zhy.android.percent.support.PercentLinearLayout
        android:id="@+id/lly_receiving_nodata"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center|top"
        android:visibility="visible">
        <ImageView
            android:id="@+id/img_reward_nodata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_widthPercent="75%sw"
            android:adjustViewBounds="true"
            android:src="@drawable/no_record"
            android:layout_marginTop="80dip" />
    </com.zhy.android.percent.support.PercentLinearLayout>

    <LinearLayout
        android:id="@+id/lly_track_select"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="40dip">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="24dip"
                android:src="@drawable/order_track_icon"
                android:adjustViewBounds="true"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/benhao"
                android:textSize="16dip"
                android:textColor="@color/black"
                android:layout_marginLeft="5dip"/>
            <TextView
                android:id="@+id/tv_tracking_box_book"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="16dip"
                android:textColor="@color/black"
                android:layout_marginLeft="10dip"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:layout_marginTop="20dip">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/final_receiver"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_one"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/ultimate_recipient_parent"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_two"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/final_receiver_status"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_three"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/ultimate_recipient_parent_status"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_four"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/final_receiver_settlement_time"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_five"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/ultimate_recipient_parent_settlement_time"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_six"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/delivery_person"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_seven"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/delivery_time"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_eight"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/administrators_winning_amount"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_night"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/ultimate_recipient_parent_winning_amount"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_ten"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/final_receiver_winning_amount"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_eleven"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:orientation="horizontal"
            android:background="@color/ghostwhite">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/total_rewards"
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginLeft="30dip"/>
            <TextView
                android:id="@+id/tv_tracking_twelve"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center|right"
                android:text=""
                android:textSize="14dip"
                android:textColor="#111111"
                android:layout_marginRight="30dip"/>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
