<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/jc_header_text_white">
    <RelativeLayout
        android:id="@+id/header_type_one_panel"
        android:layout_width="match_parent"
        android:layout_height="48dp">
        <LinearLayout
            android:id="@+id/lly_manual_scanner_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:padding="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/header_type_one_back_arrow"
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:paddingLeft="10dip"
                android:adjustViewBounds="true"
                android:src="@drawable/arrow_left_black"/>

        </LinearLayout>

        <TextView
            android:id="@+id/header_type_one_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/jackpot"
            android:textColor="#000"
            android:textSize="18sp"/>
    </RelativeLayout>
    <com.scwang.smartrefresh.layout.SmartRefreshLayout
        android:id="@+id/srl_receiving"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <com.scwang.smartrefresh.layout.header.ClassicsHeader
        android:id="@+id/header_receiving"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/cash_prize_top_bg"
                    app:layout_marginPercent="4%sw"
                    android:gravity="center">
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:id="@+id/lly_lottory_top"
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        app:layout_heightPercent="15%sh"
                        android:orientation="horizontal"
                        android:background="@drawable/cash_prize_top_bg"
                        android:gravity="center"
                        android:visibility="visible">

                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:id="@+id/lly_cash_prize_qr"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/my_balance"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="2%sw"/>
                            <TextView
                                android:id="@+id/tv_my_lottery_balance"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/white"
                                app:layout_textSizePercent="7%sw"
                                app:layout_marginTopPercent="1.5%sw"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/tv_my_lottery_money"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/wallet_details"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="3.5%sw"
                                app:layout_marginTopPercent="2.5%sw"
                                android:background="@drawable/immediate_top_btn_bg"
                                app:layout_paddingTopPercent="1.2%sw"
                                app:layout_paddingBottomPercent="1.2%sw"
                                app:layout_paddingLeftPercent="6%sw"
                                app:layout_paddingRightPercent="6%sw"
                                android:visibility="gone"/>
                            <TextView
                                android:id="@+id/tv_lottery_recharge"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/recharge"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="3.5%sw"
                                app:layout_marginTopPercent="4%sw"
                                android:background="@drawable/immediate_top_btn_bg"
                                app:layout_paddingTopPercent="1.2%sw"
                                app:layout_paddingBottomPercent="1.2%sw"
                                app:layout_paddingLeftPercent="6%sw"
                                app:layout_paddingRightPercent="6%sw"
                                android:visibility="gone"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>

                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/cash_prize_top_bg_inside"
                        app:layout_marginLeftPercent="3%sw"
                        app:layout_marginRightPercent="3%sw"
                        app:layout_marginTopPercent="3%sw"
                        android:gravity="center"
                        android:orientation="vertical">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_heightPercent="6%sh"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="0dip"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/fixed_quota"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginLeftPercent="4%sw"/>
                            <TextView
                                android:id="@+id/tv_my_lottery_fixed_quota"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/white"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginRightPercent="4%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_heightPercent="6%sh"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="0dip"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/used_quota"
                                android:textColor="#FFE700"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginLeftPercent="4%sw"/>
                            <TextView
                                android:id="@+id/tv_my_lottery_used_quota"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="#FFE700"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginRightPercent="4%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:layout_heightPercent="6%sh"
                            android:orientation="horizontal"
                            android:gravity="center_vertical">
                            <TextView
                                android:layout_width="0dip"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="@string/surplus_quota"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginLeftPercent="4%sw"/>
                            <TextView
                                android:id="@+id/tv_my_lottery_surplus_quota"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/white"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginRightPercent="4%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>

                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_heightPercent="8%sh"
                        android:orientation="horizontal"
                        android:gravity="center_vertical"
                        app:layout_marginLeftPercent="7%sw"
                        app:layout_marginRightPercent="7%sw">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/settled_amount"
                                android:textColor="@color/white"
                                app:layout_textSizePercent="3.5%sw" />
                            <TextView
                                android:id="@+id/tv_my_lottery_settle"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:textColor="@color/white"
                                app:layout_textSizePercent="3.5%sw" />
                        </com.zhy.android.percent.support.PercentLinearLayout>

                        <TextView
                            android:id="@+id/tv_my_lottery_settlement_list"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/settlement_records"
                            android:textColor="@color/white"
                            app:layout_textSizePercent="3.5%sw"
                            app:layout_marginLeftPercent="2%sw"
                            android:background="@drawable/immediate_top_btn_bg"
                            app:layout_paddingTopPercent="1.2%sw"
                            app:layout_paddingBottomPercent="1.2%sw"
                            app:layout_paddingLeftPercent="6%sw"
                            app:layout_paddingRightPercent="6%sw"
                            android:visibility="gone"/>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                </com.zhy.android.percent.support.PercentLinearLayout>

                <com.zhy.android.percent.support.PercentLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_heightPercent="36%sh"
                    android:weightSum="3"
                    android:orientation="vertical"
                    android:background="@drawable/cash_prize_lly_bg"
                    app:layout_marginLeftPercent="4%sw"
                    app:layout_marginRightPercent="4%sw"
                    android:visibility="gone">
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="总销量"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="3000.000"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="6%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="今日销量"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="3000.000"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="6%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dip"
                        android:background="@color/jc_header_text_white"
                        app:layout_marginLeftPercent="4%sw"
                        app:layout_marginRightPercent="4%sw"/>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="K3"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.00"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="match_parent"
                            android:background="@color/jc_header_text_white" />
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="37选6"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.00"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dip"
                        android:background="@color/jc_header_text_white"
                        app:layout_marginLeftPercent="4%sw"
                        app:layout_marginRightPercent="4%sw"/>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dip"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="90选5"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.00"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="match_parent"
                            android:background="@color/jc_header_text_white" />
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center|left"
                            app:layout_marginLeftPercent="6%sw"
                            android:visibility="invisible">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="90选5"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.00"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                </com.zhy.android.percent.support.PercentLinearLayout>

                <com.zhy.android.percent.support.PercentLinearLayout
                    android:id="@+id/lly_my_lottery_bottom"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/cash_prize_lly_bg"
                    app:layout_marginTopPercent="4%sw"
                    app:layout_marginLeftPercent="4%sw"
                    app:layout_marginRightPercent="4%sw">
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/rel_my_lottery"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        app:layout_marginTopPercent="2%sw">
                    </android.support.v7.widget.RecyclerView>
                    <com.zhy.android.percent.support.PercentLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:weightSum="4"
                        android:visibility="gone">
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:id="@+id/lly_my_lottery_recharge_record"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_heightPercent="9%sw"
                                app:layout_widthPercent="9%sw"
                                android:src="@drawable/my_lottory_one"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="充值记录"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:id="@+id/lly_my_lottery_cash"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:visibility="gone">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_heightPercent="9%sw"
                                app:layout_widthPercent="9%sw"
                                android:src="@drawable/my_jkc_six"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="兑奖记录"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:id="@+id/lly_my_lottery_settlement"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:visibility="gone">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_heightPercent="9%sw"
                                app:layout_widthPercent="9%sw"
                                android:src="@drawable/my_lottory_three"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="结算"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                        <com.zhy.android.percent.support.PercentLinearLayout
                            android:id="@+id/lly_my_lottery_betting"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:visibility="gone">
                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_heightPercent="9%sw"
                                app:layout_widthPercent="9%sw"
                                android:src="@drawable/my_lottory_three"/>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="投注记录"
                                android:textColor="@color/black"
                                app:layout_textSizePercent="4%sw"
                                app:layout_marginTopPercent="3%sw"/>
                        </com.zhy.android.percent.support.PercentLinearLayout>
                    </com.zhy.android.percent.support.PercentLinearLayout>
                </com.zhy.android.percent.support.PercentLinearLayout>
            </com.zhy.android.percent.support.PercentLinearLayout>

            <LinearLayout
                android:id="@+id/lly_my_immediate_pop"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:visibility="gone"
                android:background="#33000000"
                android:orientation="vertical"
                android:paddingBottom="10dip">
                <LinearLayout
                    android:id="@+id/lly_delivery_pop_one"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40dip"
                    android:layout_marginRight="40dip"
                    android:background="@drawable/pop_delivery_management_bg"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:visibility="visible">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="32dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="@string/password_verification"
                            android:textSize="14dip"
                            android:textColor="@color/white"
                            android:background="@drawable/delivery_management_lly_bg"
                            android:gravity="center"
                            android:paddingLeft="35dip"
                            android:paddingRight="35dip"/>
                        <LinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:visibility="gone">
                            <ImageView
                                android:id="@+id/img_my_immediate_no"
                                android:layout_width="26dip"
                                android:layout_height="16dip"
                                android:src="@drawable/guanbi"
                                android:paddingRight="10dip"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dip"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:orientation="vertical"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/please_enter_secondary_password"
                            android:textSize="14dip"
                            android:textColor="@color/black"/>
                        <EditText
                            android:id="@+id/et_my_immediate_password"
                            android:layout_width="match_parent"
                            android:layout_height="40dip"
                            android:textSize="14dip"
                            android:gravity="center_vertical"
                            android:layout_marginTop="10dip"
                            android:hint="@string/please_enter_secondary_password"
                            android:inputType="textPassword"
                            android:paddingLeft="20dip"
                            android:background="@drawable/delivery_input_btn_bg"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dip"
                        android:background="#CCCCCC"
                        android:layout_marginTop="20dip"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dip"
                        android:weightSum="2"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">
                        <Button
                            android:id="@+id/bt_my_immediate_pop_no"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/str_cancel"
                            android:textColor="#999999"
                            android:textSize="14dip"
                            android:background="@null"/>
                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="match_parent"
                            android:background="#CCCCCC"/>
                        <Button
                            android:id="@+id/bt_my_immediate_pop_yes"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/confirm"
                            android:textColor="@color/main_color"
                            android:textSize="14dip"
                            android:background="@null"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/lly_lock_pop"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:background="#33000000"
                android:orientation="vertical"
                android:paddingBottom="10dip"
                android:visibility="gone">
                <LinearLayout
                    android:id="@+id/lly_lock_pop_one"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/pop_delivery_management_bg"
                    android:orientation="vertical"
                    android:gravity="center_horizontal"
                    android:visibility="visible"
                    android:layout_marginLeft="40dip"
                    android:layout_marginRight="40dip">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="32dip">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="@string/password_verification"
                            android:textSize="14dip"
                            android:textColor="@color/white"
                            android:background="@drawable/delivery_management_lly_bg"
                            android:gravity="center"
                            android:paddingLeft="35dip"
                            android:paddingRight="35dip"/>
                        <LinearLayout
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center_vertical|right"
                            android:visibility="gone">
                            <ImageView
                                android:layout_width="26dip"
                                android:layout_height="16dip"
                                android:src="@drawable/guanbi"
                                android:paddingRight="10dip"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dip"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:orientation="vertical"
                        android:gravity="center">
                        <ImageView
                            android:id="@+id/img_lock_icon"
                            android:layout_width="48dip"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:src="@drawable/suo"/>
                        <TextView
                            android:id="@+id/tv_lock_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/lock_or_not"
                            android:textSize="14dip"
                            android:textColor="@color/black"
                            android:layout_marginTop="15dip"/>
                    </LinearLayout>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dip"
                        android:background="#CCCCCC"
                        android:layout_marginTop="20dip"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dip"
                        android:weightSum="2"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">
                        <Button
                            android:id="@+id/bt_lock_pop_no"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/str_cancel"
                            android:textColor="#999999"
                            android:textSize="14dip"
                            android:background="@null"/>
                        <View
                            android:layout_width="0.5dip"
                            android:layout_height="match_parent"
                            android:background="#CCCCCC"/>
                        <Button
                            android:id="@+id/bt_lock_pop_yes"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text="@string/confirm"
                            android:textColor="@color/main_color"
                            android:textSize="14dip"
                            android:background="@null"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </com.scwang.smartrefresh.layout.SmartRefreshLayout>
</LinearLayout>
