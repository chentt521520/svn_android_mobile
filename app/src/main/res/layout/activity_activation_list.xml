<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#F2F2F2">

    <RelativeLayout
        android:id="@+id/header_type_one_panel"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/main_color">
        <LinearLayout
            android:id="@+id/lly_manual_scanner_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:padding="5dip"
            android:gravity="center_vertical"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/header_type_one_back_arrow"
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:paddingLeft="10dip"
                android:adjustViewBounds="true"
                android:src="@drawable/arrow_left_white"/>
        </LinearLayout>
        <TextView
            android:id="@+id/header_type_one_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/activation_record"
            android:textColor="@color/white"
            android:textSize="18sp"/>

        <LinearLayout
            android:id="@+id/lly_reward_record_select"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dip"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="5dip"
            android:layout_alignParentRight="true">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="23dip"
                android:adjustViewBounds="true"
                android:src="@drawable/cx_w" />
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <com.zhy.android.percent.support.PercentLinearLayout
                android:id="@+id/lly_settlement_nodata"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:visibility="gone"
                android:layout_marginTop="20dip">
                <ImageView
                    android:id="@+id/img_reward_nodata"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="35%sw"
                    android:adjustViewBounds="true"
                    android:layout_gravity="center"
                    android:src="@drawable/no_record"
                    />
            </com.zhy.android.percent.support.PercentLinearLayout>
            <LinearLayout
                android:id="@+id/lly_activation_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@color/white">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dip"
                    android:orientation="horizontal"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:gravity="center_vertical"
                    android:background="@color/white"
                    android:visibility="gone">
                    <TextView
                        android:id="@+id/tv_record_num"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="1"
                        android:textSize="12dip"
                        android:textColor="@color/black"
                        android:gravity="center_vertical"
                        android:visibility="invisible"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="13dip"
                        android:adjustViewBounds="true"
                        android:src="@drawable/activation_icon"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="5dip"
                        android:visibility="invisible"/>
                    <TextView
                        android:id="@+id/tv_record_order"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="@string/ticket_number"
                        android:textSize="12dip"
                        android:textColor="@color/black"
                        android:gravity="center"/>
                    <TextView
                        android:id="@+id/tv_record_state"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="@string/activation_state"
                        android:textSize="12dip"
                        android:textColor="@color/black"
                        android:gravity="center_vertical"/>
                    <TextView
                        android:id="@+id/tv_record_time"
                        android:layout_width="0dip"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:text="@string/activation_time"
                        android:textSize="12dip"
                        android:textColor="@color/black"
                        android:gravity="center"
                        android:layout_marginRight="10dip"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="15dip"
                        android:src="@drawable/arrow_right_small"
                        android:visibility="invisible"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dip"
                    android:background="#D8D8D8"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:visibility="gone"/>
                <com.jc.lottery.view.XListView
                    android:id="@+id/xlv_settlement"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:cacheColorHint="@null"
                    android:divider="@null"
                    android:scrollbars="none"
                    android:background="#F2F2F2"/>
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/lly_receiving_select"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#99000000"
            android:orientation="vertical"
            android:visibility="gone">
            <LinearLayout
                android:id="@+id/lly_receiving_click"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/please_select_conditionss"
                    android:textColor="@color/black"
                    android:textSize="16dip"
                    android:gravity="center|left"
                    android:layout_marginTop="15dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginBottom="15dip"
                    android:visibility="gone"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:gravity="center_vertical"
                    android:visibility="gone">
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text=""
                            android:hint="@string/case_number"
                            android:textSize="14dip"
                            android:textColor="#888888"
                            android:gravity="center"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="--"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:textColor="#888888"/>
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text=""
                            android:hint="@string/benhao"
                            android:textSize="14dip"
                            android:textColor="#888888"
                            android:gravity="center"/>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:gravity="center_vertical"
                    android:visibility="gone">
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text=""
                            android:hint="@string/receiver"
                            android:textSize="14dip"
                            android:textColor="#888888"
                            android:gravity="center"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="--"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:textColor="#888888"/>
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:text=""
                            android:hint="@string/activation_state"
                            android:textSize="14dip"
                            android:textColor="#888888"
                            android:gravity="center"/>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/time_choice"
                    android:textColor="@color/black"
                    android:textSize="16dip"
                    android:gravity="center|left"
                    android:layout_marginTop="15dip"
                    android:layout_marginBottom="15dip"
                    android:layout_marginLeft="15dip"/>
                <LinearLayout
                    android:id="@+id/lly_receiving_select_container_three"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginLeft="15dip"
                    android:layout_marginRight="15dip"
                    android:gravity="center_vertical">
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/tv_receiving_select_start"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text=""
                            android:hint="@string/please_choose"
                            android:textSize="12dip"
                            android:textColor="#888888"
                            android:gravity="center"/>

                        <ImageView
                            android:layout_width="12dip"
                            android:layout_height="wrap_content"
                            android:src="@drawable/time_right"
                            android:layout_marginRight="15dip"/>
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="--"
                        android:layout_marginLeft="15dip"
                        android:layout_marginRight="15dip"
                        android:textColor="#888888"
                        android:visibility="invisible"/>
                    <LinearLayout
                        android:layout_width="0dip"
                        android:layout_height="30dip"
                        android:layout_weight="1"
                        android:background="@drawable/reward_et_bg"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/tv_receiving_select_end"
                            android:layout_width="0dip"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:text=""
                            android:hint="@string/please_choose"
                            android:textSize="12dip"
                            android:textColor="#888888"
                            android:gravity="center"/>

                        <ImageView
                            android:layout_width="12dip"
                            android:layout_height="wrap_content"
                            android:src="@drawable/time_right"
                            android:layout_marginRight="15dip"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dip"
                    android:orientation="horizontal"
                    android:layout_marginBottom="20dip"
                    android:layout_marginTop="20dip">
                    <Button
                        android:id="@+id/btn_receiving_select_reset"
                        android:layout_width="0dip"
                        android:layout_height="45dip"
                        android:layout_weight="1"
                        android:background="@drawable/scanner_shape_bg"
                        android:text="@string/reset_time"
                        android:textColor="@color/white"
                        android:textSize="16dip"
                        android:layout_marginLeft="20dip"
                        android:layout_marginRight="20dip" />

                    <Button
                        android:id="@+id/btn_receiving_select_submit"
                        android:layout_width="0dip"
                        android:layout_height="45dip"
                        android:layout_weight="1"
                        android:background="@drawable/scanner_shape_bg"
                        android:text="@string/query"
                        android:textColor="@color/white"
                        android:textSize="16dip"
                        android:layout_marginRight="20dip" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
